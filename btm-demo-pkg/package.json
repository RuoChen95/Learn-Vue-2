{
  "name": "btm-demo-pkg",
  "version": "0.1.0",
  "main": "./index.js",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-coupon-lib": "vue-cli-service build --target lib --name coupon ./src/components/Coupon.vue",
