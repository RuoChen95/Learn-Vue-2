    "html-webpack-plugin": "^3.2.0",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "vue-i18n": "^8.0.0",
    "vue-template-compiler": "^2.5.16",
    "webpack-cli": "^3.1.0"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
